@model List<BuilderWebSite.ViewModels.WebSite.ReferencesViewModel>

<section class="clients">
    <div class="container">
        <div class="row">
            <div class="section-heading">
                <h2>REFERANSLAR</h2>
                <div class="line-dec"></div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                <div id="owl-clients">
                    @foreach (var item in Model)
                    {
                        <div class="item">
                            <img src="@item.FileName" alt="">
                        </div>
                    }
                </div>
            </div>
        </div>
    </div>
</section>